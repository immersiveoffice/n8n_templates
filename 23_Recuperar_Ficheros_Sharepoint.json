{
    "name": "23_Recuperar_Ficheros_Sharepoint",
    "nodes": [
        {
            "parameters": {
                "path": "get-sharepoint-file",
                "responseMode": "lastNode",
                "options": {}
            },
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 1,
            "position": [
                0,
                0
            ],
            "id": "webhook-trigger",
            "name": "Webhook"
        },
        {
            "parameters": {
                "resource": "file",
                "operation": "download",
                "fileId": "0123456789ABCDEF",
                "siteId": "my-site-id"
            },
            "type": "n8n-nodes-base.microsoftSharePoint",
            "typeVersion": 1,
            "position": [
                200,
                0
            ],
            "id": "sharepoint-download",
            "name": "SharePoint Download"
        },
        {
            "parameters": {
                "fileName": "/data/downloaded_file.pdf",
                "options": {}
            },
            "type": "n8n-nodes-base.writeBinaryFile",
            "typeVersion": 1,
            "position": [
                400,
                0
            ],
            "id": "save-file",
            "name": "Save File"
        }
    ],
    "connections": {
        "Webhook": {
            "main": [
                [
                    {
                        "node": "SharePoint Download",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "SharePoint Download": {
            "main": [
                [
                    {
                        "node": "Save File",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    }
}